{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build/unbundled",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ 
      {
        "source" : "**/*.@(jpg|jpeg|gif|png|ico)",
        "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=86400"
        } ]
      } 
    ]
  }
}
