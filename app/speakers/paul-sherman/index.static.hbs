    - app/partials/pages/index/footer.static.hbs
---
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:100300400600700" rel="stylesheet" async>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" async>

    <style>
      {{>speaker-page-style}}
    </style>

  </head>

  <body>
    <div id="header-container">
      {{>page-nav-header}}
    </div>

    <div id="page-content">
      <div class="uxph-speaker-page">
        {{#with data.speakers}}
        <div class="speaker-side">
          <img src="{{0.boxavatar}}">
        </div>

        <div class="speaker-content">
          <section class="uxph-blue-bg-5 uxph-speaker-banner speaker-content-header">
            <div class="flex speaker-content-header-text">
              <h1>{{0.name}}</h1>
              <span class="speaker-company">{{0.shortBio}}</span>
              <div class="uxph-small-divider">
              </div>
              <h2>{{0.talkTopic}}</h2>
            </div>
            <div class="flex uxph-banner-image" >
            </div>
          </section>
          <div class="speaker-content-body">
            <div class="paul-speaker">
              <div class="speaker-short-bio">
              {{#each 0.shortBioList}}
                <span class="speaker-short-bio-title">
                  {{title}}
                </span><br/>
                <span class="speaker-short-bio-text">
                  {{body}}
                </span>
                {{#unless @last}}
                <div class="uxph-small-divider">
                </div>
                {{/unless}}
              {{/each}}
              </div>
            </div>
            <div class="speaker-wrapper"></div>
            <p>{{0.fullBio}}</p>
          </div>
        </div>
        {{/with}}
      </div>
      {{!--{{{>speaker}}
      {{>schedule}}
      {{>venue}}
      {{>gallery}}
      {{>pricing}}
      {{>faq}}--}}
      {{!--{{>contact}}--}}
      {{!--{{>about}}
      {{>sponsor}}--}}
      {{!--{{>update}}--}}
    </div>

    <div id="footer-container">
      {{>footer}}
    </div>

    <link rel="import" href="/project_components/core-app-component/core-app-component.html" >
    <core-app-component id="core-component"></core-app-component>

    <script src="/app.js">
    </script>
    <script>require('scripts/initialize.js');</script>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-86514907-1', 'auto');
      ga('send', 'pageview');
    </script>

